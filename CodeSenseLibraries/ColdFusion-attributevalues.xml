<?xml version="1.0" encoding="UTF-8"?>
<!--

CodeSense Libraries simply define lists of completions, categorized by keys. They don't define any criteria for when they should be used; this is the domain of a CodeSense Provider.

-->
<codesense version="1.0">
	
	<!-- cfargument -->
	<set name="com.adobe.cfml.tag.cfargument.attribute-values.required">
		<completion string="yes" />
		<completion string="no" />
	</set>
	
	<set name="com.adobe.cfml.tag.cfargument.attribute-values.type">
		<completion string="any" />
		<completion string="array" />
		<completion string="binary" />
		<completion string="radio" />
		<completion string="boolean" />
		<completion string="component" />
		<completion string="date" />
		<completion string="guid" />
		<completion string="numeric" />
		<completion string="query" />
		<completion string="string" />
		<completion string="struct" />
		<completion string="uuid" />
		<completion string="variableName" />
		<completion string="xml" />
		<completion string="componentName" />
	</set>
	
	<!-- cffunction -->
	<set name="com.adobe.cfml.tag.cffunction.attribute-values.access">
		<completion string="private" />
		<completion string="package" />
		<completion string="public" />
		<completion string="remote" />
	</set>
	
